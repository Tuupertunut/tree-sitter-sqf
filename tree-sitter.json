{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sqf",
      "camelcase": "Sqf",
      "title": "SQF",
      "scope": "source.sqf",
      "file-types": [
        "sqf"
      ],
      "injection-regex": "^sqf$",
      "class-name": "TreeSitterSqf"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Arma SQF language grammar for tree-sitter",
    "authors": [
      {
        "name": "Tuupertunut"
      }
    ],
    "links": {
      "repository": "https://github.com/Tuupertunut/tree-sitter-sqf"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}